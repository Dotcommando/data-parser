{
  "name": "data-parser",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open --port 8008 --watch",
    "build": "vue-cli-service build",
    "clean:watcher": "rimraf server-dist/watcher",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "css:fix": "stylelint \"src/**/*.scss\" --fix",
    "lint:js": "eslint --ext .js \".\"",
    "lint:vue": "eslint --ext .vue \".\"",
    "js:fix": "eslint --ext .js \".\" --fix",
    "vue:fix": "eslint --ext .vue \".\" --fix",
    "fix": "npm run lint:css:fix && npm run lint:js-vue:fix",
    "watcher:build-dev": "cross-env BABEL_ENV=server npm run clean:watcher && webpack --config ./watcher.config.js -w",
    "watcher:build": "cross-env BABEL_ENV=server npm run clean:watcher && webpack --config ./watcher.config.js -p",
    "watcher:dev": "cross-env-shell BABEL_ENV=server NODE_ENV=development nodemon server-dist/watcher.bundle.js",
    "watcher:prod": "cross-env-shell BABEL_ENV=server NODE_ENV=production node server-dist/watcher.bundle.js"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "http-status": "^1.3.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^4.2.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
    "koa2-cors": "^2.0.6",
    "mongoose": "^5.5.15",
    "puppeteer": "^3.0.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^2.3.0",
    "jsdoc": "^3.6.2",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.2.0",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "npm run js:fix",
      "git add"
    ],
    "**/*.scss": [
      "npm run css:fix",
      "git add"
    ]
  }
}
